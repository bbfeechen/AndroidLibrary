<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bbfeechen.gmail.com.volleysample"
    >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".util.VolleyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".view.BaseActivity">
        </activity>
        <activity android:name=".view.GsonRequestActivity">
        </activity>
        <activity android:name=".view.ImageLoadingActivity">
        </activity>
        <activity android:name=".view.JsonRequestActivity">
        </activity>
        <activity android:name=".view.ParamsRequestActivity">
        </activity>
        <activity android:name=".view.SimpleRequestActivity">
        </activity>
    </application>

</manifest>
